    "type": "library",
    "description": "Expose php constants to front.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ConstantExposureBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2",
        "symfony/cache": "^3.4|^4.0|^5.0",
